            <string>//get inputs
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_up);

//set imagespeed
image_speed = 0.5;

//actions
if (move != 0)
    old = move;

image_speed = 0.2;

// -1 if left, 1 if right
move = key_right + key_left;

if (jump == true)
{
    if (oldmove &lt; 0)
        sprite_index = JumpLeft;
    else if (oldmove &gt; 0)
        sprite_index = JumpRight;
    if (place_meeting(x,y+1,oWall))
    {
        jump = false;
    }
    move = oldmove;
}

hsp = move * movespeed;

//Sprite Change while walking
if (jump == false)
{
    if (move == 0)
    {
        if (old &lt; 0)
          sprite_index = StopLeft;
        if (old &gt; 0)
            sprite_index = StopRight;
    }
    else if (move &gt; 0)
        sprite_index = WalkRightSnow;
    else
        sprite_index = WalkLeftSnow;
}
//

//

//falling speed
if (vsp &lt; 10) vsp += grav;

//jump
if (place_meeting(x, y+1, oWall))
{
    vsp = key_jump * -jumpspeed;
}

//jumping sprite
if (key_jump)
{
    jump = true;
    oldmove = move;
}
   
//horizontal collision
if (place_meeting(x+hsp, y, oWall))
{
    while (!place_meeting(x+sign(hsp), y, oWall))
    {
        x += sign(hsp);
    }
    x += 0;
}
else
    x += hsp;
if (place_meeting(x+hsp, y, oWall) &amp;&amp; !place_meeting(x+hsp, y-16, oWall))
{
    while (place_meeting(x+hsp, y-1, oWall))
    {
        y -= 1;
    }
    y -= 1;
    x += sign(hsp);
}

//vertical collision
if (place_meeting(x, y+vsp, oWall))
{
    while (!place_meeting(x,y+sign(vsp), oWall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

y += vsp;

</string>
